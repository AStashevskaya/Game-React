{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style":"off",
        "no-param-reassign": ["error", { "props": false }],
        "jsx-a11y/no-static-element-interactions": [
            "error",
            {
             " handlers": [
                "onClick",
                "onMouseDown",
                "onMouseUp",
                "onKeyPress",
                "onKeyDown",
                "onKeyUp"
              ]
            }
          ]
    },
    "globals": {
        "localStorage": true,
        "fetch": true,
        "window": true,
        "document": true,
        "Audio": true
      }
}
